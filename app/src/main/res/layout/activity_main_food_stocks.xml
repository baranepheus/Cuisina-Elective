<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainFoodStocksActivity">

    <TextView
        android:layout_width="412dp"
        android:layout_height="180dp"
        android:text="@string/insert_picture"
        />

    <Button
        android:id="@+id/buttonFruitsActivity"
        android:layout_width="352dp"
        android:layout_height="140dp"
        android:layout_marginTop="200dp"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:text="@string/fruits"
        />

    <Button
        android:id="@+id/buttonMeatActivity"
        android:layout_width="352dp"
        android:layout_height="140dp"
        android:layout_marginTop="430dp"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:text="@string/meat"
        />

    <Button
        android:id="@+id/buttonDairyActivity"
        android:layout_width="352dp"
        android:layout_height="140dp"
        android:layout_marginTop="590dp"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:text="@string/dairy"
        />

    <Button
        android:id="@+id/buttonVegetableActivity"
        android:layout_width="352dp"
        android:layout_height="140dp"
        android:layout_marginTop="750dp"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:text="@string/vegetable"
        />

    <Button
        android:id="@+id/buttonCondimentsActivity"
        android:layout_width="352dp"
        android:layout_height="140dp"
        android:layout_marginTop="910dp"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:text="@string/condiments"
        />

</RelativeLayout>